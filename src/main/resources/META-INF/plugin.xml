<!-- Plugin Configuration File. Read more: https://plugins.jetbrains.com/docs/intellij/plugin-configuration-file.html -->
<idea-plugin>
    <!-- Unique identifier of the plugin. It should be FQN. It cannot be changed between the plugin versions. -->
    <id>org.example.demo_plugin</id>

    <!-- Public plugin name should be written in Title Case.
         Guidelines: https://plugins.jetbrains.com/docs/marketplace/best-practices-for-listing.html#plugin-name -->
    <name>Demo_plugin</name>

    <!-- A displayed Vendor name or Organization ID displayed on the Plugins Page. -->
    <vendor url="https://www.test.com">Your Company</vendor>

    <!-- Description of the plugin displayed on the Plugin Page and IDE Plugin Manager.
         Guidelines: https://plugins.jetbrains.com/docs/marketplace/best-practices-for-listing.html#plugin-description -->
    <description>Demo plugint to show all locatiosn for plgins</description>

    <!-- Product and plugin compatibility requirements.
         Read more: https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <!-- Extension points defined by the plugin.
         Read more: https://plugins.jetbrains.com/docs/intellij/plugin-extension-points.html -->
    <extensions defaultExtensionNs="com.intellij">

    </extensions>
    <actions>
        <!-- ========== MAIN MENU GROUPS ========== -->

        <!-- New Main Menu - Create anew Group Main Menu and add an action isnide -->
        <group id="com.example.demo_plugin.MyCustomMenu"
               text="(Rodrigo) Custom Menu"
               description="My custom menu"
               popup="true">
            <add-to-group group-id="MainMenu" anchor="last"/>

            <action id="com.example.demo_plugin.MainMenuAction"
                    class="com.example.demo_plugin.main_menu.MainMenuAction"
                    text="Main Menu Action"
                    description="Action in custom menu"/>
        </group>

        <!-- File Menu - Common file operations like New, Open, Save, Close -->
        <action id="com.example.demo_plugin.FileMenuAction"
                class="com.example.demo_plugin.main_menu.FileMenuAction"
                text="(Rodrigo) File Menu Action"
                description="Action in File menu">
            <add-to-group group-id="FileMenu" anchor="last"/>
        </action>


        <!-- Edit Menu - Text editing operations like Cut, Copy, Paste, Find -->
        <action id="com.example.demo_plugin.EditMenuAction"
                class="com.example.demo_plugin.main_menu.EditMenuAction"
                text="(Rodrigo) Edit Menu Action"
                description="Action in Edit menu">
            <add-to-group group-id="EditMenu" anchor="last"/>
        </action>


        <!-- View Menu - UI view options like Tool Windows, Appearance -->
        <action id="com.example.demo_plugin.ViewMenuAction"
                class="com.example.demo_plugin.main_menu.ViewMenuAction"
                text="(Rodrigo) View Menu Action"
                description="Action in View menu">
            <add-to-group group-id="ViewMenu" anchor="last"/>
        </action>


        <!-- Navigate Menu - Code navigation like Go to Class, File, Symbol -->
        <action id="com.example.demo_plugin.GoToMenuAction"
                class="com.example.demo_plugin.main_menu.GoToMenuAction"
                text="(Rodrigo) Go to Menu Action"
                description="Action in Go to menu">
            <add-to-group group-id="GoToMenu" anchor="last"/>
        </action>

        <!-- Code Menu - Code generation and analysis like Generate, Reformat -->
        <action id="com.example.demo_plugin.CodeMenuAction"
                class="com.example.demo_plugin.main_menu.CodeMenuAction"
                text="(Rodrigo) Code Menu Action"
                description="Action in Code menu">
            <add-to-group group-id="CodeMenu" anchor="last"/>
        </action>


        <!-- Refactoring Menu - Code refactoring operations like Rename, Extract -->
        <action id="com.example.demo_plugin.RefactoringMenuAction"
